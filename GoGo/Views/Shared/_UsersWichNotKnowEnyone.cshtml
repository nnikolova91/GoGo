@model System.Collections.Generic.IEnumerable<ViewModels.GoUserViewModel>

@if (Model == null)
{
<h1></h1>
}
else
{

@foreach (var destination in Model)
    {

    var base64 = Convert.ToBase64String(@destination.Image);
    var imgSrc = String.Format("data:image/gif;base64,{0}", base64);

<a asp-action="Details" asp-controller="Destinations" asp-route-id="@destination.Id" >
    @*<div class="inline">*@
        <btn>Details</btn>
        <img src="@imgSrc" />
    @*</div>*@
</a>
    }

}
